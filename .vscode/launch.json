{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Main",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Run migrate (package main)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/migrate", // points to the folder (package main)
      "cwd": "${workspaceFolder}",
      "args": ["fullreset"]
    }
  ]
}
